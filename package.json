{
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "react"
          ],
          "plugins": [
            "transform-class-properties"
          ]
        }
      ]
    ]
  },
  "scripts": {
    "start": "forever start src/server.js",
    "stop": "forever stopall",
    "start-server": "nodemon src/server.js",
    "start-dev": "parallelshell 'npm run watch' 'npm run sync'",
    "preclean": "./bin/preclean",
    "clean": "./bin/clean",
    "sync": "browser-sync start --reload-delay 500 --files 'build/**/*.css, build/**/*.js'",
    "build-vendor-js": "./bin/build-vendor-js",
    "build-js": "./bin/build-js",
    "watch-js": "./bin/watch-js",
    "build-css": "./bin/build-css",
    "watch-css": "./bin/watch-css",
    "build": "npm run clean && npm run build-vendor-js && npm run build-js && npm run build-css",
    "watch": "parallelshell 'npm run watch-js' 'npm run watch-css'"
  },
  "engines": {
    "node": "5.3.0"
  },
  "license": "MIT",
  "dependencies": {
    "babel-plugin-transform-class-properties": "^6.6.0",
    "babel-polyfill": "^6.6.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.6.5",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "dotenv": "^2.0.0",
    "es6-promise": "^3.1.2",
    "forever": "^0.15.1",
    "koa": "^1.2.0",
    "koa-body": "^1.4.0",
    "koa-compress": "^1.0.9",
    "koa-helmet": "^1.0.0",
    "koa-nunjucks-render": "^1.1.1",
    "koa-router": "^5.4.0",
    "koa-static": "^2.0.0",
    "node-fetch": "^1.3.3",
    "node-sass": "^3.4.2",
    "parallelshell": "^2.0.0",
    "ramda": "^0.19.1",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "uglify-js": "^2.6.2",
    "uglifyify": "^3.0.1",
    "whatwg-fetch": "^0.11.0"
  },
  "devDependencies": {
    "browser-sync": "^2.11.1",
    "nodemon": "^1.9.1",
    "watchify": "^3.7.0"
  }
}
